{
    "name": "@omni-oss/set-version",
    "description": "Sets version of various package manifests",
    "version": "0.1.1",
    "bin": "./dist/set-version.mjs",
    "publishConfig": {
        "access": "public"
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": {
                "development": "./src/index.ts",
                "default": "./dist/index.mjs"
            },
            "require": {
                "development": "./src/index.ts",
                "default": "./dist/index.cjs"
            }
        }
    },
    "dependencies": {
        "zod": "catalog:",
        "commander": "catalog:",
        "@commander-js/extra-typings": "catalog:",
        "@omni-oss/system-interface": "workspace:^",
        "fast-xml-parser": "^5.3.6",
        "yaml": "^2.8.2",
        "comment-json": "^4.5.1",
        "smol-toml": "^1.6.0",
        "picomatch": "^4.0.3"
    },
    "devDependencies": {
        "@omni-oss/tsconfig": "catalog:",
        "@omni-oss/vite-config": "catalog:",
        "@omni-oss/vitest-config": "catalog:",
        "vite": "catalog:",
        "vitest": "catalog:",
        "@types/node": "catalog:",
        "@types/bun": "catalog:",
        "@types/picomatch": "^4.0.2",
        "typescript": "catalog:"
    }
}
