{
    "name": "@omni-oss/set-version",
    "description": "Sets version of various package manifests",
    "version": "0.1.0",
    "bin": "./dist/cli.mjs",
    "publishConfig": {
        "access": "public"
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": {
                "development": "./src/index.ts",
                "default": "./dist/set-version.mjs"
            },
            "require": {
                "development": "./src/index.ts",
                "default": "./dist/set-version.cjs"
            }
        }
    },
    "dependencies": {
        "zod": "^4.3.6",
        "commander": "^14.0.3",
        "@commander-js/extra-typings": "14.0.0",
        "@omni-oss/system-interface": "workspace:^",
        "fast-xml-parser": "^5.3.4",
        "yaml": "^2.8.2",
        "comment-json": "^4.5.1",
        "smol-toml": "^1.6.0",
        "picomatch": "^4.0.3"
    },
    "devDependencies": {
        "@omni-oss/tsconfig": "workspace:^",
        "@omni-oss/vite-config": "workspace:^",
        "@omni-oss/vitest-config": "workspace:^",
        "vite": "^7.3.1",
        "vitest": "^4.0.18",
        "@types/node": "25.2.0",
        "@types/bun": "^1.3.8",
        "@types/picomatch": "^4.0.2",
        "typescript": "^5.9.3"
    }
}
